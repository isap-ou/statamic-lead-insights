{
    "name": "isapp/statamic-lead-insights",
    "description": "Know where your leads come from. Captures UTM parameters, referrers, and landing pages â€” attaches them to Statamic form submissions.",
    "type": "statamic-addon",
    "license": "proprietary",
    "keywords": [
        "statamic",
        "utm",
        "attribution",
        "lead",
        "analytics",
        "marketing",
        "forms"
    ],
    "authors": [
        {
            "name": "ISAPP",
            "homepage": "https://isapp.be"
        }
    ],
    "homepage": "https://github.com/isap-ou/statamic-lead-insights",
    "support": {
        "issues": "https://github.com/isap-ou/statamic-lead-insights/issues"
