            "Isapp\\LeadInsights\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Lead Insights",
            "description": "Know where your leads come from. UTM capture, referrer tracking, and attribution for Statamic forms.",
            "editions": ["free", "pro"]
        },
        "laravel": {
